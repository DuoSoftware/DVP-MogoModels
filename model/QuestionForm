var mongoose = require('mongoose');
var Schema = mongoose.Schema;
var ObjectId = Schema.ObjectId;
require('./QuestionSection');

var questionFormSchema = new Schema({

    formName: {type: String, required: true},
    sections: [{type: ObjectId, ref: 'QuestionSection'}],
    company: { type: Number, required: true },
    tenant: { type: Number, required: true }

});


var QuestionForm = mongoose.model('QuestionForm', questionFormSchema);
module.exports.QuestionForm = QuestionForm;